PACKAGE=linkchecker

dpkg --listfiles $PACKAGE |
        awk '$0~/\.py$/ {print $0"c\n" $0"o"}' |
        xargs rm -f >&2

rm -f /usr/lib/python2.0/site-packages/linkcheck
rm -f /usr/lib/python2.0/site-packages/DNS

#DEBHELPER#

exit 0
