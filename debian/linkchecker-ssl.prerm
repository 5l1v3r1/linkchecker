PYTHON=python2.1
PACKAGE=linkchecker-ssl

#DEBHELPER#

dpkg --listfiles $PACKAGE |
        awk '$0~/\.py$/ {print $0"c\n" $0"o"}' |
        xargs rm -f >&2

rmdir /usr/lib/$PYTHON/site-packages/linkcheckssl 2>/dev/null || true

exit 0
