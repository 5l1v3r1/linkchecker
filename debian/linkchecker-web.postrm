#!/bin/sh
# postrm script for linkchecker-web
#
# see: dh_installdeb(1)
set -e

#DEBHELPER#

if [ "$1" = "remove" ] || [ "$1" = "purge" ]; then
    if [ -e /usr/share/apache2/apache2-maintscript-helper ] ; then
        . /usr/share/apache2/apache2-maintscript-helper
        apache2_invoke disconf linkchecker.conf || exit $?
    fi
fi

exit 0
