#. type: Plain text
#: en/linkchecker.1:4
msgid "linkchecker - check HTML documents and websites for broken links"
msgstr ""

#. type: SH
#: en/linkchecker.1:5 en/linkchecker-gui.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: en/linkchecker.1:7
msgid "B<linkchecker> [I<options>] [I<file-or-url>]..."
