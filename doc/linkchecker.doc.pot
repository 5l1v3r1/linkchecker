#: en/linkchecker.1:201
#, no-wrap
msgid "B<--no-anchor-caching>"
msgstr ""

# type: Plain text
#: en/linkchecker.1:207
msgid ""
"Treat url#anchora and url#anchorb as equal on caching. This is the default "
